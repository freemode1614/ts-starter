{
  "name": "ts-starter",
  "version": "0.0.0",
  "author": "freemode1614 <freemode1614@outlook.com>",
  "description": "Project starter for TypeScript",
  "license": "MIT",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "npm"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsup --watch",
    "build": "cross-env NODE_ENV=production tsup",
    "test": "cross-env NODE_ENV=test vitest --watch=false",
    "fmt": "biome format --write",
    "lint": "biome lint --write",
    "check": "biome check"
  },
  "keywords": [
    "ts-starter",
    "typescript library",
    "typescript library template"
  ],
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@changesets/cli": "^2.29.8",
    "@moccona/preset-config": "^0.0.5",
    "@types/node": "^22.19.1",
    "cross-env": "^10.1.0",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.9.0",
  "resolutions": {
    "rolldown": "npm:@rolldown/browser@latest"
  }
}
