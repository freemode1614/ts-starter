{
  "name": "ts-starter",
  "version": "0.0.0",
  "author": "freemode1614 <freemode1614@outlook.com>",
  "description": "Project starter for TypeScript",
  "license": "MIT",
  "type": "module",
  "audits": true,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "npm"
  ],
  "keywords": [
    "ts-starter",
    "typescript library",
    "typescript library template"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup --minify",
    "test": "npm run test:unit && npm run test:coverage",
    "test:unit": "vitest --watch=false",
    "test:coverage": "vitest run --coverage",
    "fmt": "biome format --write",
    "lint": "biome lint --write",
    "check": "biome check",
    "prepare": "husky"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@changesets/cli": "^2.29.8",
    "@moccona/preset-config": "^0.0.5",
    "@types/node": "^22.19.1",
    "@vitest/coverage-istanbul": "3.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.9.0",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "biome format --check",
      "git add"
    ],
    "src/**/*": [
      "git add"
    ]
  }
}
