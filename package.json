{
  "name": "ts-starter",
  "version": "0.0.0",
  "description": "Project starter for TypeScript",
  "type": "module",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development tsup --watch",
    "build": "cross-env NODE_ENV=production tsup",
    "test": "cross-env NODE_ENV=test vitest --watch=false"
  },
  "keywords": [
    "ts-starter",
    "typescript library",
    "typescript library template"
  ],
  "author": "freemode1614",
  "license": "MIT",
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./npm/*.d.ts",
      "import": "./npm/*.js",
      "require": "./npm/*.cjs"
    },
    ".": {
      "types": "./npm/index.d.ts",
      "import": "./npm/index.js",
      "require": "./npm/index.cjs"
    }
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@moccona/eslint-config": "^1.0.6",
    "@moccona/preset-config": "^0.0.5",
    "@types/node": "^20.17.50",
    "cross-env": "^10.0.0",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "prettier": "@moccona/preset-config/prettierrc",
  "packageManager": "pnpm@10.9.0"
}
